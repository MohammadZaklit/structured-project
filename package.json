{
  "name": "my-library",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "format": "prettier --write \"**/*.{js,mjs,ts,mts,d.ts,html}\" --cache",
    "start": "nx serve my-first-app --ssl",
    "start:form-builder": "nx serve form-builder --ssl --port=1200",
    "admin": "nx serve admin-generator --ssl",
    "build": "nx build admin-generator",
    "watch": "nx build --watch --configuration development",
    "migrate-prefix": "ts-node tools/scripts/update-prefix.ts",
    "build:lib": "nx build my-lib",
    "test": "nx test",
    "api": "nx serve api",
    "api:build": "nx build api",
    "api:migrate:dev": "cross-env NODE_ENV=development node projects/api/db/migrate.js",
    "api:migrate:supa": "cross-env NODE_ENV=supabase node projects/api/db/migrate.js",
    "api:migrate:test": "cross-env NODE_ENV=staging node projects/api/db/migrate.js",
    "api:migrate:live": "cross-env NODE_ENV=production node projects/api/db/migrate.js",
    "api:seed": "cross-env NODE_ENV=supabase node projects/api/run-seeds.js"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
