{
  "name": "my-library",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "format": "prettier --write \"**/*.{js,mjs,ts,mts,d.ts,html}\" --cache",
    "start": "ng serve --project=my-first-app --ssl",
    "start:form-builder": "ng serve --project=form-builder --ssl",
    "start:admin-generator": "ng serve --project=admin-generator --ssl",
    "build": "ng build admin-generator",
    "watch": "ng build --watch --configuration development",
    "build:lib": "ng build my-lib",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.3",
    "@angular/cdk": "20.2.3",
    "@angular/common": "20.3.3",
    "@angular/compiler": "20.3.3",
    "@angular/core": "20.3.3",
    "@angular/forms": "20.3.3",
    "@angular/platform-browser": "20.3.3",
    "@angular/platform-server": "20.3.3",
    "@angular/router": "20.3.3",
    "@primeuix/themes": "^1.2.3",
    "@supabase/supabase-js": "^2.57.4",
    "@tailwindcss/cli": "^4.1.13",
    "@types/uuid": "^10.0.0",
    "chart.js": "4.4.2",
    "primeclt": "^0.1.5",
    "@tailwindcss/postcss": "^4.1.11",
    "bootstrap": "^5.3.8",
    "dragula": "^3.7.3",
    "font-awesome": "^4.7.0",
    "ng2-dragula": "^6.0.0",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.1.2",
    "rxjs": "^7.8.2",
    "tailwindcss": "^4",
    "tailwindcss-primeui": "latest",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "20.3.3",
    "@angular/cli": "20.3.3",
    "@angular/compiler-cli": "20.3.3",
    "@types/dragula": "^2.1.34",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.1",
    "prettier": "^3.6.2",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "ng2-dragula": {
      "@angular/core": "20.3.3",
      "@angular/animations": "20.3.3",
      "@angular/common": "20.3.3"
    }
  }
}
